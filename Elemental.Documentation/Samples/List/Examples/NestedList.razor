
@* Currently nesting can be implemented through nested 
    AeList elements so this is no longer useful although
    can be good practice for understanding the c# data structures
<AeList itemList="_listItems" />

@code {
    @using Elemental.Code;

    private List<AeListds> _listItems = new List<AeListds>
    {
        new AeListds()
        {
            itemName = "Home",
        },
        new AeListds()
        {
            itemName = "Home",
        },
        
        new AeListds()
        {
            itemName = "Dropdown",
            childList = new List<AeListds>()
            {
                new AeListds()
                {
                    itemName = "Link 1"
                },
                new AeListds()
                {
                    itemName = "Link 2"
                },
                new AeListds()
                {
                    itemName = "Link 3"
                },
            }
        },
        *@
    };
}